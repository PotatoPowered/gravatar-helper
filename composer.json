{
  "name": "potatopowered/gravatar-helper",
  "type": "cakephp-plugin",
  "description": "A helper that can be used to pull profile pictures from the gravatar service for CakePHP 3.x ",
  "keywords": [
    "gravatar",
    "avatar",
    "helper",
    "CakePHP"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Blake Sutton",
      "email": "sutton.blake@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^5.4.16|^7.2",
    "cakephp/cakephp": "^3.0|^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0|^8.0",
    "cakephp/cakephp-codesniffer": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "GravatarHelper\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GravatarHelper\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
